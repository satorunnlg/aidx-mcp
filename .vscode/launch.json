{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AIDX MCP Server (venv)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/client/mcp-server/src/main.py",
      "cwd": "${workspaceFolder}/client/mcp-server/src",
      "env": {
        "AIDX_CAD_TYPE": "fusion360",
        "AIDX_PORT": "8109"
      },
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "AIDX Test Client",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/client/test-client/src/test_protocol.py",
      "cwd": "${workspaceFolder}/client/test-client/src",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "AutoCAD AIDX Addin (Attach)",
      "type": "coreclr",
      "request": "attach",
      "processName": "acad.exe"
    }
  ]
}
